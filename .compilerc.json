{
  "env": {
    "development": {
      "application/javascript": {
        "passthrough": true
      },
      "text/typescript": {
        "module": "CommonJS",
        "target": "es6",
        "sourceMap": true
      }
    },

    "production": {
      "application/javascript": {
        "passthrough": true
      },
      "text/typescript": {
        "sourceMap": false
      }
    }
  }
}