{
  "name": "electronts",
  "version": "1.0.0",
  "description": "Electron Typescript Bootstrap Boilerplate",
  "main": "dist/lib/App.js",
  "scripts": {
    "start": "npm-run-all --sequential build electron",
    "electron": "cross-env NODE_ENV=dev electron .",
    "ts": "tsc",
    "html": "cd dist && mkdir views && cd .. && cp src/views/*.html dist/views",
    "clean": "rm -rf dist/",
    "build": "npm-run-all --sequential clean build-all html",
    "build-all": "npm-run-all --parallel ts css",
    "css": "mkdir dist && cd dist && mkdir css && cd .. && uglifycss src/css/Index.css > dist/css/Index.css"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/hammus/electron-ts.git"
  },
  "author": "Liam Whan",
  "license": "MIT",
  "devDependencies": {
    "@types/bootstrap": "^4.1.0",
    "@types/jquery": "^3.3.2",
    "cross-env": "^5.1.6",
    "electron": "^2.0.2",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.1",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  }
}
