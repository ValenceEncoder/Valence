{
  "name": "MKV2ATV",
  "version": "0.0.1",
  "description": "MKV to AppleTV (MKV2ATV) is a simple cross platform gui application that can speed up encoding video for AppleTV by using FFMPEG to preserve (copy) x264/h264 encoded video streams stored in MKV containers and converting them to MP4 containers.",
  "main": "src/App.ts",
  "scripts": {
    "cli": "cross-env tsc && node dist/FFCLI.js",
    "start": "electron-forge start",
    "test": "nyc mocha",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hammus/MKV2ATV.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "private": true,
  "keywords": [
    "video",
    "encoding",
    "mkv",
    "mp4",
    "appletv",
    "converter",
    "container",
    "Electron",
    "typscript"
  ],
  "author": "Liam Whan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hammus/MKV2ATV/issues"
  },
  "homepage": "https://github.com/hammus/MKV2ATV#readme",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/electron": "^1.4.37",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.1.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^4.0.0",
    "gulp": "^3.9.1",
    "i": "^0.3.5",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.2",
    "tslint": "latest",
    "typescript": "^2.3.2",
    "electron-compile": "^6.4.1",
    "electron-debug": "^1.1.0",
    "electron-prebuilt-compile": "1.6.6"
  },
  "dependencies": {
    "command-line-args": "^4.0.2",
    "config": "^1.25.1",
    "devtron": "^1.4.0",
    "hammerjs": "^2.0.4",
    "materialize-css": "^0.98.2"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {},
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "hammus",
        "name": "MKV2ATV"
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
