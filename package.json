{
  "name": "Valence",
  "productName": "Valence",
  "version": "0.1.1-alpha",
  "description": "Valence is a simple GUI application for converting video files",
  "main": "src/App.ts",
  "scripts": {
    "test": "nyc mocha",
    "start": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "package-darwin": "electron-forge package --platform darwin",
    "package-win": "electron-forge package --platform win32",
    "package-nix": "electron-forge package --platform linux --arch all",
    "make-darwin": "electron-forge make --platform darwin",
    "make-win": "electron-forge make --platform win32 --arch all",
    "make-linux32": "electron-forge make --platform linux --arch ia32",
    "make-linux64": "electron-forge make --platform linux --arch x64",
    "make-linux": "npm run make-linux32 && npm run make-linux64",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src"
  },
  "keywords": [
    "mkv",
    "converter",
    "appletv",
    "apple-tv",
    "mp4",
    "ffmpeg",
    "handbrake",
    "video"
  ],
  "author": "Liam Whan",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "node_modules/**/*"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "forge": {
      "github_repository": {
        "owner": "ValenceEncoder",
        "name": "Valence",
        "draft": true,
        "prerelease": true
      },
      "publish_targets": {
        "win32": [
          "github"
        ]
      },
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "dmg",
          "zip"
        ],
        "linux": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "ignore": [
          ".idea",
          "src/vendor/materialize/fonts",
          "src/vendor/materialize/sass",
          "dist",
          "vid",
          "test",
          "install.sh",
          "Gulpfile.js",
          "license-concat.js",
          ".nvmrc",
          ".gitignore",
          ".nyc_output",
          "logo/ValenceLogo.ai"
        ],
        "icon": "build/icon.icns",
        "electronVersion": "1.6.6",
        "overwrite": true
      },
      "electronWinstallerConfig": {
        "name": "Valence",
        "setupExe": "valence-encoder-setup.exe",
        "description": "Valence is a graphical interface video encoder that uses Electron and FFMPEG."
      },
      "electronInstallerDebian": {},
      "electronInstallerDMG": {
        "icon": "build/icon.icns",
        "background": "build/background.png",
        "overwrite": true,
        "contents": [
          {
            "x": 336,
            "y": 80,
            "type": "link",
            "path": "/Applications"
          },
          {
            "x": 80,
            "y": 80,
            "type": "file",
            "path": "/Users/liam/Documents/Valence/out/Valence-darwin-x64/Valence.app"
          }
        ]
      },
      "electronInstallerRedhat": {},
      "windowsStoreConfig": {
        "packageName": "",
        "name": "valence"
      }
    }
  },
  "dependencies": {
    "@types/chai": "^3.5.2",
    "@types/decimal.js": "0.0.30",
    "@types/electron": "^1.4.37",
    "@types/materialize-css": "^0.98.0",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.1.3",
    "command-line-args": "^4.0.2",
    "decimal.js": "^7.2.1",
    "electron-compile": "^6.4.1",
    "electron-debug": "^1.1.0",
    "electron-node-config": "^1.0.1",
    "gulp-uglifycss": "^1.0.8",
    "hammerjs": "^2.0.4",
    "jquery": "2.2.4",
    "materialize-css": "^0.98.2"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "electron-prebuilt-compile": "1.6.6",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-copy": "^1.0.0",
    "gulp-install": "^1.1.0",
    "gulp-json-modify": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-typescript": "^3.1.6",
    "run-sequence": "^1.2.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "nyc": "^10.3.0",
    "chai": "^3.5.0",
    "ts-node": "^3.0.2"
  }
}
