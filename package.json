{
  "name": "MKV2ATV",
  "version": "0.0.1",
  "description": "MKV to AppleTV (MKV2ATV) is a simple cross platform gui application that can speed up encoding video for AppleTV by using FFMPEG to preserve (copy) x264/h264 encoded video streams stored in MKV containers and converting them to MP4 containers.",
  "main": "src/App.ts",
  "scripts": {
    "cli": "cross-env tsc && node dist/FFCLI.js",
    "start": "cross-env NODE_ENV=development electron .",
    "test": "jasmine-ts spec/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hammus/MKV2ATV.git"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "private": true,
  "keywords": [
    "video",
    "encoding",
    "mkv",
    "mp4",
    "appletv",
    "converter",
    "container",
    "Electron",
    "typscript"
  ],
  "author": "Liam Whan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hammus/MKV2ATV/issues"
  },
  "homepage": "https://github.com/hammus/MKV2ATV#readme",
  "devDependencies": {
    "@types/electron": "^1.4.37",
    "@types/electron-packager": "latest",
    "@types/jasmine": "^2.5.47",
    "@types/promise": "^7.1.30",
    "cross-env": "^4.0.0",
    "devtron": "^1.4.0",
    "electron-builder": "latest",
    "electron-debug": "^1.1.0",
    "electron-packager": "latest",
    "electron-prebuilt-compile": "^1.6.2",
    "gulp": "^3.9.1",
    "i": "^0.3.5",
    "jasmine": "^2.6.0",
    "jasmine-node": "^1.14.5",
    "jasmine-spec-reporter": "^4.1.0",
    "jasmine-ts": "^0.1.0",
    "ts-node": "^3.0.2",
    "tslint": "latest",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "command-line-args": "^4.0.2",
    "config": "^1.25.1",
    "jquery": "^3.2.1",
    "materialize-css": "^0.98.2",
    "mkv-theme": "hammus/mkv-theme#1.0.0",
    "promise": "^7.1.1"
  }
}
